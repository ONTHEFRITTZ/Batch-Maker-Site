{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/subscription/(.*)",
      "dest": "/api/subscription.js"
    },
    {
      "src": "/webhook/(.*)",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/network/(.*)",
      "dest": "/api/network.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}